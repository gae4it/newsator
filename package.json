{
  "name": "newsator",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:netlify": "netlify dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "check": "eslint . --cache && prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md,mdx,yaml,yml}\" && tsc --noEmit",
    "fix": "eslint . --cache --fix && prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md,mdx,yaml,yml}\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@netlify/functions": "^5.1.2",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.20",
    "baseline-browser-mapping": "^2.9.19",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "netlify-cli": "^23.13.5",
    "postcss": "^8.4.49",
    "prettier": "^3.7.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.48.0",
    "vite": "^6.4.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md,mdx,yaml,yml}": [
      "prettier --write"
    ]
  }
}
